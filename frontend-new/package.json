{
  "name": "frontend-new",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.97",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "~2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "axe-playwright": "^1.2.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "concurrently": "~8.2.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "http-status-codes": "~2.2.0",
    "jest": "~29.5.0",
    "jest-extended": "~4.0.2",
    "prettier": "^3.0.3",
    "run-script-os": "~1.1.6",
    "supertest": "~6.3.3",
    "ts-jest": "~29.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@material-ui/styles": "^4.11.5",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.15.12",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "prettier": "@tabiya/prettier-config",
  "scripts": {
    "lint": "eslint src --ext .ts --ext .tsx --max-warnings 0",
    "clean": "rimraf -I build coverage storybook-static",
    "compile": "tsc --noEmit",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "test:smoke": "jest --config jest.smoke.config.js --runInBand test/smoke/*.test.ts",
    "postinstall:win32": "cd .. & link-tabiya-prettier-config.bat",
    "postinstall:darwin:linux": "(cd .. && ./link-tabiya-prettier-config.sh)",
    "postinstall": "run-script-os",
    "format:check": "prettier --check src/",
    "format": "prettier --write src/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "import/first": "off"
    }
  },
  "jest": {
    "resetMocks": false,
    "clearMocks": false,
    "transformIgnorePatterns": [
      "node_modules/(?!p-limit)/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/**/_test_utilities/*",
      "!**/**/*.d.ts",
      "!**/**/*.types.ts"
    ],
    "coverageReporters": [
      [
        "lcov",
        {
          "projectRoot": "../"
        }
      ],
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
