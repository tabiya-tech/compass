    "typescript": "^5.4.5",
    "wait-for": "~2.1.1",
    "webpack": "^5.91.0"
  },
  "resolutions": {
    "@storybook/test-runner/playwright": "^1.49.1"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@mui/system": "^7.3.7",
    "@react-pdf/renderer": "^3.4.5",
    "@sentry/cli": "^2.39.1",
    "@sentry/react": "^9.41.0",
    "@sentry/webpack-plugin": "^2.22.6",
    "@types/js-yaml": "^4.0.9",
    "brotli-wasm": "^3.0.1",
    "colorjs.io": "^0.5.2",
    "docx": "^8.5.0",
    "firebase": "^10.12.2",
    "firebaseui": "^6.1.0",
    "framer-motion": "^11.2.11",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "jwt-decode": "^4.0.0",
    "lodash.debounce": "^4.0.8",
    "notistack": "^3.0.1",
    "react": "18.3.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "~18.2.0",
    "react-i18next": "^14.1.2",
    "react-router-dom": "^6.23.1",
    "react-scripts": "^5.0.1",
    "react-swipeable": "^7.0.2",
    "source-map-explorer": "^2.5.3",
    "web-vitals": "^2.1.4",
    "yaml": "^2.7.0"
  },
  "scripts": {
    "analyze": "npx source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src --ext .ts --ext .tsx --max-warnings 0",
    "clean": "rimraf -I build coverage storybook-static",
    "compile": "tsc --noEmit",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "serve": "npx http-server build",
    "test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "test:smoke": "jest --config jest.smoke.config.js --runInBand test/smoke/*.test.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test:accessibility": "npx concurrently -k -s first -n 'STORYBOOK,TEST' -c 'magenta,blue' 'npx http-server storybook-static --port 6007 --silent' 'npx wait-on http://127.0.0.1:6007 && yarn test-storybook --url http://127.0.0.1:6007'",
    "format:check": "prettier --check src/",
    "format": "prettier --write src/",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org tabiya --project compass-frontend ./build && sentry-cli sourcemaps upload --org tabiya --project compass-frontend ./build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "rules": {
      "import/first": "off"
    }
  },
  "jest": {
    "resetMocks": false,
    "clearMocks": false,
    "transformIgnorePatterns": [
      "node_modules/(?!p-limit)/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/**/_test_utilities/*",
      "!src/**/*.stories.{ts,tsx}",
      "!**/**/*.d.ts",
      "!**/**/*.types.ts",
      "!src/Report/ReportDocx/**/*"
    ],
    "coverageReporters": [
      [
        "lcov",
        {
          "projectRoot": "../"
        }
      ],
      "text"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
